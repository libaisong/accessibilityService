<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#aae0ffff" />
            <stroke android:width="1.0dip" android:color="#ff0f2c77" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#ffe0ffff" />
            <stroke android:width="1.0dip" android:color="#ff0f2c77" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>